{
    "manifest_version": 2,
    "name": "show kittens to me",
    "version" : "0.1",
    "web_accessible_resources":[
        "kittens/*.jpg"
       
    ],
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js" : ["kitten.js"]
        
    }],
    "background" : {
        "scripts":["background.js"]
    },
    "browser_action" :{
        "default_icon" : "cat.png"
    }
    

}